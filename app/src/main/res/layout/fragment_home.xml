                        android:id="@+id/imageComic63"
                        android:layout_width="60dp"
                        android:layout_height="60dp"
                        android:src="@drawable/comicportada" />

                    <LinearLayout
                        android:id="@+id/gasto13"
                        android:layout_width="wrap_content"
